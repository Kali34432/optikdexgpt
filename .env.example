# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Stripe Configuration
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# OpenAI API Configuration
VITE_OPENAI_API_KEY=your_openai_api_key_here

# Admin Configuration
VITE_ADMIN_EMAIL=admin@optikcoin.com
VITE_ADMIN_SECRET_KEY=your_admin_secret_key_here

# API Configuration
VITE_API_BASE_URL=https://api.optikcoin.com
VITE_ADMIN_API_URL=https://admin-api.optikcoin.com

# Security Configuration
VITE_JWT_SECRET=your_jwt_secret_here
VITE_ENCRYPTION_KEY=your_encryption_key_here

# External Services
VITE_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
VITE_SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Analytics & Monitoring
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# Email Configuration
VITE_SMTP_HOST=smtp.gmail.com
VITE_SMTP_PORT=587
VITE_SMTP_USER=your_email@gmail.com
VITE_SMTP_PASS=your_email_password

# Storage Configuration
VITE_AWS_ACCESS_KEY=your_aws_access_key
VITE_AWS_SECRET_KEY=your_aws_secret_key
VITE_AWS_BUCKET=optikdex-storage
VITE_AWS_REGION=us-east-1

# Rate Limiting
VITE_RATE_LIMIT_REQUESTS=1000
VITE_RATE_LIMIT_WINDOW=3600

# Feature Flags
VITE_ENABLE_MINING=true
VITE_ENABLE_STAKING=true
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_ADMIN_PANEL=true

# Supabase Edge Functions Environment Variables (for deployment)
# These should be set in your Supabase dashboard under Settings > Edge Functions

# STRIPE_SECRET_KEY=your_stripe_secret_key
# STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
# SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key